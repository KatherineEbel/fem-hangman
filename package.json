{
  "name": "fem-hangman",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "pnpm run build && wrangler pages deploy",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "local:migrations:apply": "wrangler d1 migrations apply fem-hangman --local",
    "local:d1:init": "pnpm run local:migrations:apply && wrangler d1 execute fem-hangman --file='./app/drizzle/seed.sql' --local",
    "remote:d1:init": "pnpm run local:migrations:apply && wrangler d1 execute fem-hangman --file='./app/drizzle/seed.sql' --remote",
    "migrations:gen": "drizzle-kit generate --out ./app/drizzle/migrations --schema ./app/drizzle/schema.server.ts --dialect sqlite",
    "migrations:up": "drizzle-kit up:sqlite",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "pnpm run build && wrangler pages dev",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@conform-to/react": "^1.2.2",
    "@conform-to/zod": "^1.2.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/adapter-d1": "^5.22.0",
    "@prisma/client": "^5.22.0",
    "@remix-run/cloudflare": "^2.13.1",
    "@remix-run/cloudflare-pages": "^2.13.1",
    "@remix-run/react": "^2.13.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.36.1",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241106.0",
    "@remix-run/dev": "^2.13.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.28.0",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "3.57.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
